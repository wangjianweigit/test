	              </if>
	              <if test="public_user_type != null and public_user_type == 6">
	                  and exists(select 1 from tbl_store_user_rel tsui where tsui.store_id = tui.store_id and tsui.user_id = #{public_user_id,jdbcType=INTEGER})
