	</select>
	<!-- 交易趋势-访客数 折线图 -->
	<select id="r_queryTrend_VisitorCount_Chart" parameterType="java.util.Map" resultType="java.util.Map">
		select to_char(dt.date_value,'yyyy-mm-dd') create_date,count(distinct t.member_key) visitor_count
