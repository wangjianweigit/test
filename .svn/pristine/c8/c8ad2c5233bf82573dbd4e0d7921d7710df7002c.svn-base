		 where state = 2
		 order by sortid desc
	</select>
	<!-- 排序获取某一页的品牌列表-支付金额 -->
	<select id="r_queryPaymentMoney_Brand" parameterType="java.util.Map" resultType="string">
		select id
		  from (
		select t.id,nvl(t1.payment_money,0) payment_money
		  from qx_dic_product_brand t,(
		select t1.brand_id,nvl(sum(t.product_money),0) payment_money
		  from fact_order_product_sku t,dim_product t1,dim_date dt
		 where t.product_key = t1.product_key
		   and t.payment_date_key = dt.date_key
           and dt.date_value >= to_date(#{start_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and dt.date_value &lt;= to_date(#{end_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and t1.brand_id is not null
		   group by t1.brand_id) t1
		   where t.id = t1.brand_id(+)
		     and t.state = 2
		   order by ${sort} ${sort_by})
	</select>
	<!-- 排序获取某一页的品牌列表-支付金额占比 -->
	<select id="r_queryMoneyRatio_Brand" parameterType="java.util.Map" resultType="string">
		select id
		  from (
		select t.id,nvl(t1.money_ratio,0) money_ratio
		  from qx_dic_product_brand t,(
		select t1.brand_id,
			   nvl(sum(t.product_money),0)/#{allPayMoney,jdbcType=INTEGER} money_ratio
		  from fact_order_product_sku t,dim_product t1,dim_date dt
		 where t.product_key = t1.product_key
		   and t.payment_date_key = dt.date_key
           and dt.date_value >= to_date(#{start_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and dt.date_value &lt;= to_date(#{end_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and t1.brand_id is not null
		   group by t1.brand_id) t1
		   where t.id = t1.brand_id(+)
		     and t.state = 2
		   order by ${sort} ${sort_by})
	</select>
	<!-- 排序获取某一页的品牌列表-支付商品数 -->
	<select id="r_querySaleCount_Brand" parameterType="java.util.Map" resultType="string">
		select id
		  from (
		select t.id,nvl(t1.sale_count,0) sale_count
		  from qx_dic_product_brand t,(
		select t1.brand_id,nvl(sum(t.product_count),0) sale_count
		  from fact_order_product_sku t,dim_product t1,dim_date dt
		 where t.product_key = t1.product_key
		   and t.payment_date_key = dt.date_key
           and dt.date_value >= to_date(#{start_date,jdbcType=VARCHAR},'yyyy-mm-dd')
