	<!-- 排序获取某一页的品牌列表-支付买家数 -->
	<select id="r_queryPurchaseNumber_Brand" parameterType="java.util.Map" resultType="string">
		select id
		  from (
		select t.id,nvl(t1.purchase_number,0) purchase_number
		  from qx_dic_product_brand t,(
		select t1.brand_id,count(distinct t.member_key) purchase_number
		  from fact_order_product_sku t,dim_product t1,dim_date dt
		 where t.product_key = t1.product_key
		   and t.payment_date_key = dt.date_key
           and dt.date_value >= to_date(#{start_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and dt.date_value &lt;= to_date(#{end_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and t1.brand_id is not null
		   group by t1.brand_id) t1
		   where t.id = t1.brand_id(+)
		     and t.state = 2
		   order by ${sort} ${sort_by})
	</select>
	<!-- 排序获取某一页的品牌列表-支付转化率 -->
	<select id="r_queryZhl_Brand" parameterType="java.util.Map" resultType="string">
		select id
		  from (
		select t.id,case when nvl(t2.purchase_number,0) = 0 or nvl(t1.visitor_count,0) = 0  then 0
					else nvl(t2.purchase_number,0)/nvl(t1.visitor_count,0) end zhl
		  from qx_dic_product_brand t,(
		select t1.brand_id,count(distinct t.member_key) visitor_count
		  from fact_user_log_product t,dim_product t1,dim_date dt
		 where t.product_key = t1.product_key
		   and t.log_date_key = dt.date_key
           and dt.date_value >= to_date(#{start_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and dt.date_value &lt;= to_date(#{end_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and t1.brand_id is not null
		   group by t1.brand_id) t1,(
		select t1.brand_id,count(distinct t.member_key) purchase_number
		  from fact_order_product_sku t,dim_product t1,dim_date dt
		 where t.product_key = t1.product_key
		   and t.payment_date_key = dt.date_key
           and dt.date_value >= to_date(#{start_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and dt.date_value &lt;= to_date(#{end_date,jdbcType=VARCHAR},'yyyy-mm-dd')
		   and t1.brand_id is not null
		   group by t1.brand_id) t2
		 where t.id = t1.brand_id(+)
		   and t.id = t2.brand_id(+)
		   and t.state = 2
		 order by ${sort} ${sort_by})
	</select>
