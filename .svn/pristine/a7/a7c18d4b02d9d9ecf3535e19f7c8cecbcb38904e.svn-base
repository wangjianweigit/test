			select a.*,rownum num from (
	              select
	              	   tusc.id,
	              	   tusc.user_id,
	              	   tusc.state,
	                   ui.login_name,
	                   ui.user_manage_name,
	                   ui.user_manage_mobilephone,
	                   (select store_name from tbl_store_info where id=ui.store_id) as store_name,
