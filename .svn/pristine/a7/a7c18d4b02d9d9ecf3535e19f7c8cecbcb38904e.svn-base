	                   </if>
	                   <if test="public_user_type != null and public_user_type == 6">
	                       and EXISTS(SELECT 1 FROM TBL_STORE_USER_REL TSUI WHERE TSUI.STORE_ID = ui.STORE_ID AND TSUI.USER_ID = #{public_user_id,jdbcType=INTEGER})
